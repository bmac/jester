{
  "name": "jester",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "test": "vitest",
    "e2e": "NODE_OPTIONS='--import tsx/esm' cucumber-js --import features/support/steps.ts",
    "specflow": "NODE_OPTIONS='--import tsx/esm' cucumber-js --import features/support/steps-specflow.ts",
    "playwright": "playwright test e2e",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve ./build/server/index.js",
    "prettier": "prettier --write .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@octokit/graphql": "^8.1.1",
    "@remix-run/node": "^2.9.2",
    "@remix-run/react": "^2.9.2",
    "@remix-run/serve": "^2.9.2",
    "html-escaper": "^3.0.3",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.23.8",
    "zodix": "^0.4.4"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.7.0",
    "@mswjs/interceptors": "^0.29.1",
    "@playwright/test": "^1.44.1",
    "@remix-run/dev": "^2.9.2",
    "@resvg/resvg-js": "^2.6.2",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "cucumber-tsflow": "^4.4.4",
    "eslint": "^8.38.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "playwright": "^1.44.1",
    "prettier": "^3.2.5",
    "tsx": "^4.11.0",
    "typescript": "~5.3.0",
    "vcr-test": "^1.0.9",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
